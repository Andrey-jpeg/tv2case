<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sdu.sem2.se17.presentation.consumersystem.ConsumersysController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <StackPane prefHeight="63.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="searchField" />
                  <Button fx:id="searchButton" mnemonicParsing="false" onAction="#displayCredits" text="Vis kreditering(er)" StackPane.alignment="CENTER_RIGHT" />
               </children>
            </StackPane>
            <TableView fx:id="predictionTable" prefHeight="125.0" prefWidth="600.0">
               <columns>
                  <TableColumn fx:id="name" maxWidth="600.0" prefWidth="600.0" text="Navn" />
               </columns>
            </TableView>
            <WebView fx:id="tvTidView" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
